        <JDBCSampler guiclass="TestBeanGUI" testclass="JDBCSampler" testname="Cliente que mas productos ha comprado" enabled="true">
          <stringProp name="dataSource">conexiones</stringProp>
          <stringProp name="queryType">Select Statement</stringProp>
          <stringProp name="query">select customers.cus_name , SUM(prod_X_venta) as prodsCliente
from invoices
inner join
	(select count(*) as prod_X_venta, invoice_number
	from invoices_detail 
	group by invoice_number 
	order by prod_X_venta asc)b
on invoices.invoice_number = b.invoice_number

inner join customers
on customers.cus_id = invoices.cus_id

group by invoices.cus_id
order by prodsCliente desc
limit 1;</stringProp>
